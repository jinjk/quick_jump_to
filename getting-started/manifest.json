{
  "name": "Quick Jump",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "clipboardRead",
    "contextMenus",
    "storage"
  ],
  "commands": {
    "goto-new-page": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Run \"foo\" on the current page."
    }
  },
  "options_page": "options.html"
}