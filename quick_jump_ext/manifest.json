{
  "name": "Quick Jump",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "clipboardRead",
    "contextMenus",
    "storage"
  ],
  "commands": {
    "goto-new-page": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Goto a page with key in clipboard"
    }
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}